    SELECT 3 AS id,
           'preprod-denodo-9.iqzsystems.io' AS d_environment,
           'db2user.' AS d_schema,
           'DIMCUSTOMER' AS d_table
    FROM dual() CONTEXT ('formatted' = 'yes');

